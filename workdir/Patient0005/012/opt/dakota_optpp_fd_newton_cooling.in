interface,
        fork                                    #1,#4,#5,#7,#8,#11,#16,#19
          file_save file_tag
          analysis_driver = 'python ./brainsearch.py --param_file '
          parameters_file = './workdir/Patient0005/012/opt/optpp_pds.in'
          results_file    = './workdir/Patient0005/012/opt/optpp_pds.out'

# data quality   0

responses,
        objective_functions = 1
        numerical_gradients
        no_hessians
