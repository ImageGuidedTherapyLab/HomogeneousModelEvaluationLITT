model,
        single

variables,
        continuous_design = 4
          initial_point            0.0        21.0           0.5           6.0   
          lower_bounds             0.0        20.0           0.4           3.0   
          upper_bounds             1.e4       40.0           0.7           9.0   
          descriptors      'robin_coeff' 'probe_init' 'k_0_healthy' 'w_0_healthy'
        continuous_state =  9
          initial_state        8.e3           0.9            5.e1                 0.114300          0.123940           0.000000         0.000000 90.000000 75.000000 
          #lower_bounds        1.e3           0.7            5.e1                 0.113300          0.122940           -0.001000         -5.000000 85.000000 70.000000 
          #upper_bounds        5.e4           1.0            6.e2                 0.115300          0.124940           0.001000         5.000000 95.000000 80.000000 
          descriptors    'mu_s_healthy' 'anfact_healthy'  'mu_a_healthy'    'x_displace' 'y_displace' 'z_displace' 'x_rotate' 'y_rotate' 'z_rotate'
